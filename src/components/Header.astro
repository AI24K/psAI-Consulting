---
	import { Image } from 'astro:assets';

	const { logoAnchorTheme } = Astro.props;

    import '../css/header.css'

	import LogoText from './svg/LogoText.astro';
	import Stripe from './Stripe.astro';

	import DefineSVG from '../components/svg/services/DefineSVG.astro'
	// import AlignSVG from '../components/svg/services/AlignSVG.astro'
	import FlowSVG from '../components/svg/services/FlowSVG.astro'
	import RunSVG from '../components/svg/services/RunSVG.astro'
	import Worm from './svg/graphics/Worm.astro';
	
	import ContactBox from './ContactBox.astro';
